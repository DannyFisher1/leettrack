{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "fs:default",
    {
      "identifier": "http:default",
      "allow": [
        {
          "url": "https://leetcode-api-pied.vercel.app/*"
        }
      ]
    },
    "shell:allow-open"
  ]
}